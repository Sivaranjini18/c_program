#include <stdio.h>


int main()
{
    
int no;
    
printf("Enter your number\n");
   
 scanf("%d",&no);
   
 if(no<0)
   
 {
      
 printf("The given number is negative\n");

    }

    else if(no>0)

    {
        
printf("THe given number is positive\n");
    
}
    
else(no==0)
   
 {
       
 printf("The given number is 0\n");
    
}
    
	

}

